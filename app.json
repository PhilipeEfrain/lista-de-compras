{
  "expo": {
    "name": "Listou",
    "slug": "listou",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/shopping-card-svgrepo-com.png",
    "description": "Aplicativo Listou - Gerenciador inteligente de listas de compras com categorização, histórico e análise de gastos",
    "keywords": ["lista de compras", "compras", "supermercado", "economia", "organização", "categorias", "análise de gastos", "histórico", "gerenciador", "planejamento", "orçamento familiar"],
    "primaryColor": "#1976d2",
    "scheme": "listou",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shopping-card-svgrepo-com.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.philipe_gonzalez.listou",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/shopping-card-svgrepo-com.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shopping-card-svgrepo-com.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4b25b9ba-2e7b-41ce-8787-113b5b0a9d30"
      }
    }
  }
}
