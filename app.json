{
  "expo": {
    "name": "Listou",
    "slug": "lista-de-compras",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/shopping-card-svgrepo-com.png",
    "description": "Aplicativo Listou - Gerenciador inteligente de listas de compras com categorização, histórico e análise de gastos",
    "primaryColor": "#1976d2",
    "scheme": "listou",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shopping-card-svgrepo-com.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.philipe_gonzalez.listou",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/shopping-card-svgrepo-com.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shopping-card-svgrepo-com.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-8227454086945331~3137771321",
          "ios_app_id": "ca-app-pub-8227454086945331~3137771321",
          "delay_app_measurement_init": true,
          "optimize_initialization": true,
          "optimize_ad_loading": true,
          "user_tracking_usage_description": "Este aplicativo usa anúncios para manter o serviço gratuito."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4b25b9ba-2e7b-41ce-8787-113b5b0a9d30"
      }
    }
  }
}
